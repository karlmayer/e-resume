<!doctype html>
<html>

<head>

	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

	<title>{{#resume.basics}}{{name}} Resume{{/resume.basics}}</title>

	{{! work around VSCode handlebar mangling formatting}}
	{{style css}}

	<style media="print">
		body {
			font-size: 12px;
		}
	</style>

	<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,300' rel='stylesheet' type='text/css'>
</head>

<body>

	<div id="resume">

		{{#resume.basics}}
		<section class="heading">
			<header>
				<h1>{{name}}</h1>
				<div class="contact">
					{{#if email}}
					<span class="detail">
						<a href="mailto:{{email}}">{{email}}</a>
					</span>
					{{/if}}
					{{#if phone}}
					<span class="detail">
						{{phone}}
					</span>
					{{/if}}
					{{#if location}}
						<br/>
						<span class="detail">
						{{#location}}
							{{city}}, {{region}}, {{countryCode}}
						{{/location}}
						</span>
					{{/if}}					
					{{#if website}}
					<span class="detail">
						<a href="{{website}}" target="#">{{website}}</a>
					</span>
					{{/if}}
				</div>
			</header>


			{{#if summary}}
			<p>{{paragraphSplit summary}}</p>
			{{/if}}

			{{#if profiles.length}}
			{{#each profiles}}
			<div class="profile">
				{{#if network}}
				<strong class="network">
					{{network}}
				</strong>
				{{/if}}
				{{#if username}}
				<span class="username">
					{{#if url}}
					<a href="{{url}}" target="#">{{username}}</a>
					{{else}}
					{{username}}
					{{/if}}
				</span>
				{{/if}}
			</div>
			{{/each}}
			<p></p>
			{{/if}}

		</section>
		{{/resume.basics}}

		{{#if resume.skills.length}}
		<section id="skills">
			<h2>Skills</h2>
			{{#each resume.skills}}
			<div class="item">
				{{#if name}}
				<div class="name">
					{{name}}
				</div>
				{{/if}}
				{{#if keywords.length}}
				<div class="keywords">
					{{#each keywords}}
					{{.}}{{#if @last}}{{else}},{{/if}}
					{{/each}}
				</div>
				{{/if}}
			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.work.length}}
		<section class="experience">
			<h2>Experience</h2>
			{{#each resume.work}}
				{{#if breakBefore}}
					<section class="item page-break">
				{{else}}
					<section class="item">
				{{/if}}
				<header>
				{{#if position}}
					<h3 class="name">
						{{position}}
					</h3>
				{{/if}}
				</header>
				{{#if name}}
					<span class="position">
						{{name}}
					</span>
				{{/if}}
				{{#if startDate}}
				<span class="startDate">
					{{date startDate}}
				</span>
				{{/if}}
				{{#if endDate}}
				<span class="endDate">
					- {{date endDate}}
				</span>
				{{else}}
				<span class="endDate">
					- Present
				</span>
				{{/if}}

				{{#if summary}}
				<div class="summary">
					<p>{{paragraphSplit summary}}</p>
				</div>
				{{/if}}
				{{#if highlights.length}}
				<ul class="highlights">
					{{#each highlights}}
					<li>{{.}}</li>
					{{/each}}
				</ul>
				{{/if}}
			</section>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.publications.length}}
		<section id="publications">
			<h2>Publications</h2>
			{{#each resume.publications}}
			<div class="item">
				{{#if url}}
					<a href="{{url}}" target="#">
				{{/if}}
				{{#if name}}
					<span class="publication">{{name}}</span>
				{{/if}}
				{{#if url}}
					</a>
				{{/if}}
				{{#if publisher}}
					<span class="publisher">{{publisher}}</span>
				{{/if}}
				{{#if releaseDate}}
					<span class="startDate">
						{{date releaseDate}}
					</span>
				{{/if}}
				{{#if summary}}
				<div class="summary">
					<p>{{summary}}</p>
				</div>
				{{/if}}
			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.awards.length}}
		<section id="awards">
			<h2>Awards</h2>
			{{#each resume.awards}}
			<div class="item">
				{{#if title}}
					<span class="title">{{title}}</span>
				{{/if}}
				{{#if awarder}}
					<span class="awarder">{{awarder}}</span>
				{{/if}}
				{{#if date}}
					<span class="date">{{date date}}</span>
				{{/if}}
				{{#if summary}}
					<div class="summary">
						<p>{{summary}}</p>
					</div>
				{{/if}}
			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.volunteer.length}}
		<section class="experience page-break">
			<h2>Volunteering</h2>
			{{#each resume.volunteer}}
			<div class="item">
				<header>
					{{#if organization}}
					<h3 class="name">{{organization}}</h3>
					{{/if}}
				</header>
				{{#if position}}
				<span class="position">
					{{position}}
				</span>
				{{/if}}

				{{#if startDate}}
				<span class="startDate">
					{{date startDate}}
				</span>
				{{/if}}
				{{#if endDate}}
				<span class="endDate">
					- {{date endDate}}
				</span>
				{{else}}
				<span class="endDate">
					- Present
				</span>
				{{/if}}

				{{#if summary}}
				<div class="summary">
					<p>{{summary}}</p>
				</div>
				{{/if}}
				{{#if highlights.length}}
				<ul class="highlights">
					{{#each highlights}}
					<li>{{.}}</li>
					{{/each}}
				</ul>
				{{/if}}
			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.education.length}}
		<section id="education">
			<h2>Education</h2>
			{{#each resume.education}}
			<div class="item">
				<header>
					{{#if institution}}
					<h3 class="name">{{institution}}</h3>
					{{/if}}
				</header>

				<div class="dates">
					{{#if startDate}}
					<span class="startDate">
						{{date startDate}}
					</span>
					{{/if}}
					{{#if endDate}}
					<span class="endDate">
						- {{date endDate}}
					</span>
					{{else}}
					<span class="endDate">
						- Present
					</span>
					{{/if}}
				</div>

				<div class="qualification">
					{{#if studyType}}
					<span class="studyType">
						{{studyType}}
					</span>
					{{/if}}

					{{#if area}}
					<span class="area">
						- {{area}}
					</span>
					{{/if}}

					{{#if gpa}}
					<span class="gpa">
						GPA: {{gpa}}
					</span>
					{{/if}}
				</div>

			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.certificates.length}}
		<section id="education">
			<h2>certificates</h2>
			<ul>
				{{#each resume.certificates}}
				<li>
					{{#if date}}
					<span class="date">
						{{date date}} -
					</span>
					{{/if}}

					{{#if url}}
					<a href="{{url}}" target="#">
						{{/if}}
						{{#if name}}
						{{name}}
						{{/if}}
						{{#if url}}
					</a>
					{{/if}}
					{{#if description}}
					<span class="description">
						- {{description}}
					</span>
					{{/if}}
				</li>
				{{/each}}
			</ul>
			<p></p>
		</section>
		{{/if}}

		{{#if resume.languages.length}}
		<section id="languages">
			<h2>Languages</h2>
			{{#each resume.languages}}
			<div class="item">
				{{#if language}}
				<div class="language">
					{{language}}
				</div>
				{{/if}}
				{{#if fluency}}
				<div class="fluency">
					<em>{{fluency}}</em>
				</div>
				{{/if}}
			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.interests.length}}
		<section id="interests">
			<h2>Interests</h2>
			{{#each resume.interests}}
			<div class="item">
				{{#if name}}
				<div class="name">
					{{name}}
				</div>
				{{/if}}
				{{#if keywords.length}}
				<ul class="keywords">
					{{#each keywords}}
					<li>{{.}}</li>
					{{/each}}
				</ul>
				{{/if}}
			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#if resume.references.length}}
		<section id="references">
			<h2>References</h2>
			{{#each resume.references}}
			<div class="item">
				{{#if reference}}
				<blockquote class="reference">
					{{reference}}
				</blockquote>
				{{/if}}
				{{#if name}}
				<div class="name">
					â€” {{name}}
				</div>
				{{/if}}
			</div>
			{{/each}}
		</section>
		{{/if}}

		{{#resume.meta}}
			<section id="meta">
				For programmers &amp; data geeks, my resume is <a href="{{url}}">available as a YAML data file</a> and rendered using <a href="https://jsonresume.org/">https://jsonresume.org/</a>
			</section>
		{{/resume.meta}}
	</div>
</body>

</html>
